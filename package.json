{
  "name": "keycloak-angular",
  "version": "7.2.0",
  "description": "Easy Keycloak setup for Angular applications",
  "scripts": {
    "contributors:add": "node ./node_modules/all-contributors-cli/dist/cli add",
    "contributors:generate": "node ./node_modules/all-contributors-cli/dist/cli generate",
    "clean": "node ./node_modules/shx/lib/cli rm -rf ./dist/",
    "build": "npm run clean && node ./node_modules/@angular/cli/bin/ng build keycloak-angular --prod && node ./node_modules/shx/lib/cli cp ./README.md ./dist/keycloak-angular/",
    "test": "node ./node_modules/@angular/cli/bin/ng test keycloak-angular",
    "test:ci": "node ./node_modules/@angular/cli/bin/ng test keycloak-angular -- --watch=false --no-progress --browsers=ChromeHeadlessNoSandbox",
    "lint": "node ./node_modules/@angular/cli/bin/ng lint"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mauriciovigolo/keycloak-angular.git"
  },
  "author": {
    "name": "Mauricio Gemelli Vigolo",
    "email": "@mauriciovigolo",
    "url": "https://github.com/mauriciovigolo"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mauriciovigolo/keycloak-angular/issues"
  },
  "homepage": "https://github.com/mauriciovigolo/keycloak-angular#readme",
  "keywords": [
    "angular",
    "angular 4",
    "angular 5",
    "angular 6",
    "angular 7",
    "angular 8",
    "authentication",
    "authorization",
    "keycloak",
    "keycloak-js"
  ],
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.801.0",
    "@angular-devkit/build-ng-packagr": "^0.801.0",
    "@angular/cli": "^8.1.0",
    "@angular/common": "^8.1.0",
    "@angular/compiler": "^8.1.0",
    "@angular/compiler-cli": "^8.1.0",
    "@angular/core": "^8.1.0",
    "@angular/language-service": "^8.1.0",
    "@angular/platform-browser": "^8.1.0",
    "@angular/platform-browser-dynamic": "^8.1.0",
    "@angular/router": "^8.1.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "all-contributors-cli": "^6.7.0",
    "codelyzer": "^5.0.0",
    "core-js": "^2.5.4",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "keycloak-js": "^9.0.0",
    "ng-packagr": "^5.3.0",
    "protractor": "~5.4.0",
    "rxjs": "~6.4.0",
    "shx": "^0.3.2",
    "ts-node": "~7.0.0",
    "tsickle": "^0.35.0",
    "tslib": "^1.9.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3",
    "zone.js": "~0.9.1"
  }
}
